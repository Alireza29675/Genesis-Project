{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "no-unused-expression": false,
    "no-console": false
  },
  "linterOptions": {
    "exclude": [
      "**/*.js",
      "public/**/*.js",
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
